<h1>New Farmer's Market</h1>
<%= form_with url: '/farmers_markets', method: :post, local: true do |f| %>
  <%= f.label :name, "Name" %>
  <%= f.text_field :name %>

  <%= f.label :local, "Local" %>
  <%= f.text_field :local %>

  <%= f.label :vendor_count, "Vendor Count" %>
  <%= f.text_field :vendor_count %>

  <%= f.submit 'Submit' %>
<% end %>