<h1>Edit <%= @produce.name %></h1>

<%= form_with url: "/produces/#{@produce.id}", method: :patch, local: true do |f| %>
  <%= f.label :name, "Name" %>
  <%= f.text_field :name %>

  <%= f.label :seasonal, "Seasonal (true/false)" %>
  <%= f.text_field :seasonal %>

  <%= f.label :quantity, "Quantity" %>
  <%= f.text_field :quantity %>

  <%= f.submit 'Update Produce' %>
<% end %>