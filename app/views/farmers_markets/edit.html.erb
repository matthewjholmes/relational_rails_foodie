<h1>Edit <%= @farmers_market.name %></h1>

<%= form_with url: "/farmers_markets/#{@farmers_market.id}", method: :patch, local: true do |f| %>
  <%= f.label :name, "Name" %>
  <%= f.text_field :name %>

  <%= f.label :local, "Local" %>
  <%= f.text_field :local %>

  <%= f.label :vendor_count, "Vendor Count" %>
  <%= f.text_field :vendor_count %>

  <%= f.submit 'Submit' %>
<% end %>
