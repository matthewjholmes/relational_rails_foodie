<h1>New Produce for <%= @farmers_market.name %></h1>

<%= form_with url: "/farmers_markets/#{@farmers_market.id}/produces", method: :post, local: true do |f| %>
  <%= f.label :name, "Name" %>
  <%= f.text_field :name %>

  <%= f.label :seasonal, "Seasonal (true/false)" %>
  <%= f.text_field :seasonal %>

  <%= f.label :quantity, "Quantity" %>
  <%= f.text_field :quantity %>

  <%= f.submit 'Create New Produce' %>
<% end %>
