<h1>New Restaurant</h1>
<%= form_with url: '/restaurants', method: :post, local: true do |f| %>
  <%= f.label :name, "Name" %>
  <%= f.text_field :name %>

  <%= f.label :dine_in, "Dine In (true/false)" %>
  <%= f.text_field :dine_in %>

  <%= f.label :rating, "Rating" %>
  <%= f.text_field :rating %>

  <%= f.submit 'Submit'  %>
<% end %>